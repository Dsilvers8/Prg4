public void printPascal(int n) {
    if(n < 0)
        return;
    printPascal(n - 1);
    System.out.println();
    for (int k = 0; k <= n; k++)
    {
        System.out.print(binom(n, k) + " ");
    }
   
}


public int binom(int n, int k)	{
	if (k==n || k==0)
		return 1;
	else return binom(n-1,k-1) + binom(n-1, k);

}


}
